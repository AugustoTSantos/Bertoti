package semestre_2;

    public class Comunicação {
        public String usuario;
        public String senha;
        public String nick;

        public Comunicação(String usuario, String senha, String nick)
            
        {
        this.usuario = usuario;
        this.senha = senha;
        this.nick = nick;
        }

        public String get_usuario() {
            return usuario;
        }

        public void set_usuario(String novo_usuario) {
            usuario = novo_usuario;
        }


        public String get_senha() {
            return senha;
        }

        public void set_senha(String nova_senha) {
            senha = nova_senha;
        }


        public String get_nick() {
            return nick;
        }

        public void set_nick(String novo_nick) {
            nick = novo_nick;
        }
    

            
        public static void main(String[] args) {
            Comunicação user1 = new Comunicação("Bela", "2532235", "Bella");
            Comunicação user2 = new Comunicação("Garry", "3664346", "wsd");


            System.out.println("Nome: " + user1.usuario + "\n" +
            "Número de negociações: " + user1.senha + "\n" +
            "Média de dias com veiculo: " + user1.nick + "\n \n");

            System.out.println("Nome: " + user2.usuario + "\n" +
            "Número de negociações: " + user2.senha + "\n" +
            "Média de dias com veiculo: " + user2.nick + "\n \n");
        }
    }
